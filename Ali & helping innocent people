def is_valid_tag(tag):
    for i in range(0, 8, 2):
        if (int(tag[i]) + int(tag[i+1])) % 2 != 0:
            return False
    vowels = {'A', 'E', 'I', 'O', 'U', 'Y'}
    if tag[2] in vowels:
        return False
    return True
tag = input().strip()
if is_valid_tag(tag):
    print("valid")
else:
    print("invalid")
